{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "studio": "npx prisma studio --config ./prisma.config.ts",
    "restart": "npm run db:wipe && npm run dev",
    "db:wipe": "npx tsx src/lib/prisma-wipe.ts",
    "db:up": "npx prisma migrate dev && npx prisma generate",
    "grpc": "node src/lib/client/script.js",
    "setup": "npm run db:up && npm run grpc"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.2",
    "@grpc/proto-loader": "^0.8.0",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "dotenv": "^17.2.3",
    "elkjs": "^0.11.0",
    "google-protobuf": "^4.0.1",
    "grpc-tools": "^1.13.1",
    "jotai": "^2.15.2",
    "next": "^15.5.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "ts-proto": "^2.8.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.19.28",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.5.4",
    "prisma": "^7.2.0",
    "protolint": "^0.56.4",
    "tailwindcss": "^4",
    "typescript": "^5.9.3"
  }
}
